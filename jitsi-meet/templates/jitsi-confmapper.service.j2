[Unit]
Description=Jitsi Conference Mapper
After=network.target

[Service]
Type=simple
User={{ jitsi_meet_user }}
Group={{ jitsi_meet_user }}
PrivateTmp=true
PrivateDevices=true
ProtectHome=true
ProtectSystem=full
Restart=on-failure
StartLimitInterval=0
RestartSec=30
ExecStart={{ jitsi_meet_root_dir }}/confmapper/daemon.py

[Install]
WantedBy=multi-user.target
