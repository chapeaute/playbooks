# This params need to be setted at install time
#
# It's require you to proceed to the following task 
# - dump databases
# - drop databases
# - drop every ibdata* files
# - recreate the databases
# - reimport the dump

[mysqld]

innodb_file_per_table = {{mysql_config_innodb_file_per_table}}
innodb_log_file_size = {{mysql_config_innodb_log_file_size}}
innodb_log_buffer_size = {{mysql_config_innodb_log_buffer_size}}
innodb_data_file_path = {{mysql_config_innodb_data_file_path}}
innodb_autoextend_increment = {{mysql_config_innodb_autoextend_increment}}
