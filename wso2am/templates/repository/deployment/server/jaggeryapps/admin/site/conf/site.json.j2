{
  "theme": {
    "base": "wso2",
    "subtheme": "modern"
  },
  "context": "/admin",
  "request_url": "READ_FROM_REQUEST",
  "tasksPerPage": 10,
  "allowedPermission": "/permission/admin/manage/apim_admin",
  "workflows": {
    "workFlowServerURL": "https://<BPSHost>:<BPSPort>/services/",
  },
  "ssoConfiguration": {
    "enabled": "{{ wso2am_sso_enable }}",
    "issuer": "API_WORKFLOW_ADMIN",
    "identityProviderURL" : "{{ wso2am_sso_url }}/samsamlsso",
    "keyStoreName" : "/var/lib/wso2am/repository/resources/security/wso2carbon.jks",
    "keyStorePassword" : "{{ wso2am_keystore_pass }}",
    "identityAlias" : "wso2carbon",
    "verifyAssertionValidityPeriod": "true",
    "audienceRestrictionsEnabled": "true",
    "responseSigningEnabled":"false",
    "assertionSigningEnabled":"false",
    "assertionEncryptionEnabled": "false",
    "signRequests" : "true",
    "idpInit" : "false",
    "loginUserNameAttribute" : "",
    "acsURL" : "{{ wso2am_url }}/admin/jagg/jaggery_acs.jag", //use only if Assertion Consumer Service URL needs to be sent along with SAML request
    "idpInitSSOURL" : "{{ wso2am_sso_url }}/samlsso?spEntityID=API_WORKFLOW_ADMIN",
    //"externalLogoutPage" : "https://192.168.121.238:9443/samlsso?slo=true",
  },
  "reverseProxy": {
    "enabled": false,
    // values true , false , "auto" - will look for  X-Forwarded-* headers
    "host": "sample.proxydomain.com",
    // If reverse proxy do not have a domain name use IP
    "context": ""
    //"regContext":"" // Use only if different path is used for registry
  },
  "whiteListedHostNames" : ["localhost"],
  "enableLogAnalyzer" : false
}
